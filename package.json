{
  "name": "calf",
  "displayName": "Calf",
  "description": "A new type of programming ",
  "version": "1.0.7",
  "icon": "icon.png",
  "publisher": "WhiteBoxIdeas",
  "preview": false,
  "engines": {
    "vscode": "^1.74.3"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "react",
    "hierarchy tree",
    "react components",
    "parent-child",
    "navigation",
    "visualizer"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
		"viewsContainers": {
            "panel": [
                {
                    "id": "myPanel",
                    "title": "Calf",
                    "icon": "src/media/icon.png"
                }
            ]
        },
		"views": {
			"myPanel": [
				{
					"type": "webview",
					"id": "calf",
					"name": "Calf Panel"
				}
			]
		},
    "commands": [
      
      {
        "command": "calf.start",
        "title": "Calf: Show Panel"
      },
      {
        "command": "calf.startStatusBar",
        "title": "Calf: Show StatusBar"
      }
    ] 
  },
  "extensionPack": [
    "docsmsft.docs-markdown",
    "docsmsft.docs-preview"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config webpack.config.ts",
    "watch": "webpack --mode development --config webpack.config.ts --watch",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.18.6",
    "@types/babel__core": "^7.20.0",
    "@types/chai": "^4.3.4",
    "@types/dagre": "^0.7.48",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.10.5",
    "@types/object-path": "^0.11.0",
    "@types/rimraf": "^4.0.5",
    "@types/webpack": "^5.28.5",
    "@vscode/test-electron": "^2.2.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.3.7",
    "copy-webpack-plugin": "^11.0.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@coreui/icons": "^3.0.0",
    "@coreui/icons-react": "^2.1.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^5.0.8",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/vscode": "^1.75.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "dagre": "^0.8.5",
    "fs-extra": "^11.2.0",
    "object-path": "^0.11.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "11.10.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1"
  }
}
